      "source": [
        "lst = [1, 2, 3, 4, 5]\n",
        "summ = 0\n",
        "\n",
        "for i in lst:\n",
        "  summ += i\n",
        "\n",
        "avg = summ / len(lst)\n",
        "\n",
        "print('Avg of given List is',avg)"
